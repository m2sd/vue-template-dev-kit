{
  "name": "vue-template-dev-kit",
  "description": "A webpack setup to generate static templates with styleguide.",
  "version": "0.1.0",
  "author": "Michael Marcenich <michael.marcenich@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot --config config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config config/webpack.prod.js",
    "styleguide": "vue-styleguidist server --open --config config/styleguide.js",
    "styleguide:build": "cross-env NODE_ENV=styleguide vue-styleguidist build --config config/styleguide.js",
    "lint": "eslint --ext .js,.vue src",
    "lint-autofix": "eslint --ext .js,.vue src --fix",
    "lint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "precommit": "npm run lint-check && npm run lint"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-carousel": "^0.13.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "file-loader": "^1.1.4",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-webpack-plugin": "^2.2.0",
    "media-query-plugin": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^3.0.0",
    "prerender-spa-plugin": "^3.4.0",
    "prettier": "^1.14.2",
    "sass-loader": "^6.0.6",
    "stylelint": "^9.5.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "vue-loader": "^15.4.1",
    "vue-styleguidist": "^1.8.4",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.17.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  }
}
